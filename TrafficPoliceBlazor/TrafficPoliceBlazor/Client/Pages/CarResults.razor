@page "/CarResults"
@inject HttpClient Http
@inject NavigationManager NavigationManager
@using TrafficPoliceBlazor.Client.Components

<div class="SuroundCarResultsList">
    <h3>CarResults</h3>     
            <!-- reiterating through the cars array and displaying it on page in a table -->
    <SearchResultsTableLink tableContent="@cars" columnsToDisplay='new string[]{"number_plate", "brand", "model", "colour", "owner"}' LinkPrefix="/CarOwner?id=" SearchColumn="owner" />

    <div class="CarResultsListButton">
        <button class="button1" @onclick="Goback">Go Back</button>
    </div>
</div>

