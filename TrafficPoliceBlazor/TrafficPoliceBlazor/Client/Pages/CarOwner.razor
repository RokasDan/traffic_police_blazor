@page "/CarOwner"
@inject HttpClient Http
@inject NavigationManager NavigationManager
@using TrafficPoliceBlazor.Client.Components


<AuthorizeView>

    <NotAuthorized>
        <CriminalScum />
    </NotAuthorized>

    <Authorized>
        <div class="CarOwnerSuroundList">
            <h1>Car's Owner</h1>

            <SearchResultsTableLink tableContent="@people" columnsToDisplay='new string[]{"people_id", "address", "date_of_birth", "first_name", "last_name", "license_number"}' LinkPrefix="/PersonCars?id=" SearchColumn="people_id" />

            <div class="CarOwnerListButton">
                <button class="button1" @onclick="Goback">Go Back</button>
            </div>
        </div>
    </Authorized>
</AuthorizeView>




